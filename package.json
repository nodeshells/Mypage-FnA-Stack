{
  "name": "angular6-express4-typescritp2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "npm": "npm",
    "firebase": "firebase",
    "start": "npm-run-all -s build:server -p start:*",
    "start:client": "ng serve --aot=false --progress=false --proxy-config proxy.conf.json --base-href /",
    "start:server": "run-p watch:server boot:server",
    "watch:server": "tsc -w -p ./server/tsconfig.server.json",
    "watch:server_test": "tsc -w -p ./server_test/tsconfig.server_test.json",
    "boot:server": "nodemon ./dist/server/bin/www.js",
    "boot:server_test": "nodemon ./dist/server_test/test.server.js",
    "build": "run-s build:server build:client",
    "build:client": "ng build --output-path=./dist/server/public --prod --build-optimizer --aot",
    "build:server": "tsc -p ./server/tsconfig.server.json",
    "build:server_test": "tsc -p ./server/tsconfig.server.json",
    "buildRun": "run-s build boot:server",
    "serve": "npm-run-all -s build:client firebase:serve",
    "deploy": "npm-run-all -s build:client firebase:deploy",
    "hosting:kill": "firebase hosting:disable",
    "firebase:serve": "firebase serve",
    "firebase:deploy": "firebase deploy",
    "test": "run-p test:*",
    "test:client": "ng test",
    "test:server": "npm-run-all -s build:server_test -p watch:server_test  boot:server_test",
    "e2e": "npm-run-all -s  webdriver:update -p webdriver:start protractor",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "protractor": "protractor protractor.conf.js",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.6",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.1.4",
    "@angular/http": "^6.1.4",
    "@angular/material": "^6.4.6",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/router": "^6.1.4",
    "@mdi/font": "^2.7.94",
    "angularfire2": "^5.0.0-rc.12",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.7",
    "express": "^4.16.3",
    "firebase": "^5.4.2",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.2.9",
    "ngx-materialize": "^6.1.3",
    "npm-run-all": "^4.1.3",
    "rxjs": "^6.2.2",
    "tslint-config-semistandard": "^7.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.1.4",
    "@angular/compiler-cli": "^6.1.4",
    "@angular/language-service": "^6.1.4",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.9.2",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "nodemon": "^1.18.3",
    "protractor": "^5.3.2",
    "supertest": "^3.1.0",
    "ts-node": "^6.0.5",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  }
}
