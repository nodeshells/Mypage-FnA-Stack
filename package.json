{
  "name": "angular6-express4-typescritp2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "npm": "npm",
    "firebase": "firebase",
    "start": "npm-run-all -s build:server -p start:*",
    "start:client": "ng serve --aot=false --progress=false --proxy-config proxy.conf.json --base-href /",
    "start:client_nohost": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:server": "run-p watch:server boot:server",
    "watch:server": "tsc -w -p ./server/tsconfig.server.json",
    "watch:server_test": "tsc -w -p ./server_test/tsconfig.server_test.json",
    "boot:server": "nodemon ./dist/server/bin/www.js",
    "boot:server_test": "nodemon ./dist/server_test/test.server.js",
    "build": "run-s build:server build:client",
    "build:client": "ng build --output-path=./dist/server/public --prod",
    "build:server": "tsc -p ./server/tsconfig.server.json",
    "build:server_test": "tsc -p ./server/tsconfig.server.json",
    "buildRun": "run-s build boot:server",
    "serve": "npm-run-all -s build:client firebase:serve",
    "deploy": "npm-run-all -s build:client firebase:deploy",
    "hosting:kill": "firebase hosting:disable",
    "firebase:serve": "firebase serve",
    "firebase:deploy": "firebase deploy --only hosting",
    "tunnel:start": "ssh -R 8420:localhost:4200 serveo.net",
    "test": "run-p test:*",
    "test:client": "ng test",
    "test:server": "npm-run-all -s build:server_test -p watch:server_test  boot:server_test",
    "e2e": "npm-run-all -s  webdriver:update -p webdriver:start protractor",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "protractor": "protractor protractor.conf.js",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@mdi/font": "^3.7.95",
    "@types/jquery": "^3.3.30",
    "angularfire2": "^5.2.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "firebase": "^6.3.0",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.6.4",
    "ngx-color": "^3.0.3",
    "ngx-materialize": "^6.1.3",
    "npm-run-all": "^4.1.3",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "tslint-config-semistandard": "^8.0.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "^8.1.1",
    "@types/google.analytics": "0.0.40",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.6.2",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^1.19.1",
    "protractor": "^5.3.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "typescript": "3.5.3"
  }
}
